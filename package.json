{
  "name": "redux-white-black",
  "version": "0.2.5",
  "description": "Redux middleware to execute a callback on action types using a whitelist or blacklist approach",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "prepublish": "npm run build",
    "start": "npm run build",
    "test": "mocha test/ --recursive --compilers js:babel-register",
    "version": "version-changelog CHANGELOG.md && git add CHANGELOG.md"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "redux",
    "middleware",
    "whitelist",
    "blacklist"
  ],
  "repository": "https://github.com/rnsloan/redux-white-black.git",
  "author": "rnsloan1@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "np": "^2.13.1",
    "redux": "^3.5.1",
    "sinon": "^2.1.0",
    "version-changelog": "^2.0.1"
  }
}
